<head>
    <!-- Basic Meta Tags -->
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <!-- SEO Meta Tags -->
    <meta name="description" content="CSE Motors - Professional automotive services and vehicle solutions. Browse our inventory and find your perfect vehicle today.">
    <meta name="author" content="Gabriel Chikwendu Nwofoke">
    <meta name="keywords" content="cars, vehicles, automotive, motors, dealership, inventory">
    
    <!-- Theme Color for Mobile Browsers -->
    <meta name="theme-color" content="#1a365d">
    
    <!-- Title with dynamic variable -->
    <title><%= title %> | CSE Motors</title>
    
    <!-- Favicon -->
    <link rel="icon" type="image/x-icon" href="/images/favicon.ico">
    <link rel="shortcut icon" href="/images/logo.png" type="image/x-icon">
    <link rel="apple-touch-icon" sizes="180x180" href="/images/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/images/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/images/favicon-16x16.png">
    
    <!-- Open Graph / Facebook -->
    <meta property="og:type" content="website">
    <meta property="og:url" content="<%= baseURL %>/">
    <meta property="og:title" content="<%= title %> | CSE Motors">
    <meta property="og:description" content="Professional automotive services and vehicle solutions">
    <meta property="og:image" content="<%= baseURL %>/images/og-image.jpg">
    <meta property="og:image:width" content="1200">
    <meta property="og:image:height" content="630">
    <meta property="og:image:alt" content="CSE Motors Logo">
    
    <!-- Twitter -->
    <meta property="twitter:card" content="summary_large_image">
    <meta property="twitter:url" content="<%= baseURL %>/">
    <meta property="twitter:title" content="<%= title %> | CSE Motors">
    <meta property="twitter:description" content="Professional automotive services and vehicle solutions">
    <meta property="twitter:image" content="<%= baseURL %>/images/twitter-image.jpg">
    
    <!-- Preconnect for Performance -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700&family=Open+Sans:wght@400;600&display=swap" rel="stylesheet">
    
    <!-- Canonical URL -->
    <link rel="canonical" href="<%= baseURL %>/">
    
    <!-- CSS -->
    <link rel="stylesheet" href="/css/styles.css">
    
    <!-- JavaScript -->
    <script src="/js/script.js" defer></script>
</head>